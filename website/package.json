{
  "scripts": {
    "examples": "node ../Docusaurus/lib/copy-examples.js",
    "start": "npm-run-all --parallel scss-watch dc-start",
    "build": "npm-run-all scss-build dc-build copy-robot",
    "publish-gh-pages": "npm-run-all scss-build dc-publish-gh-pages copy-robot",
    "write-translations": "node ../Docusaurus/lib/write-translations.js",
    "version": "node ../Docusaurus/lib/version.js",
    "rename-version": "node ../lib/Docusaurus/rename-version.js",
    "crowdin-upload": "crowdin --config ../crowdin.yaml upload sources --auto-update -b master",
    "crowdin-download": "crowdin --config ../crowdin.yaml download -b master",
    "dc-start": "node ../Docusaurus/lib/start-server.js",
    "dc-start-debug": "node  --inspect ../Docusaurus/lib/start-server.js",
    "dc-build": "node ../Docusaurus/lib/build-files.js",
    "dc-publish-gh-pages": "node ../Docusaurus/lib/publish-gh-pages.js",
    "copy-robot": "cp ./robots.txt ./build/sqlgate-docs/robots.txt",
    "scss-build": "gulp scss",
    "scss-watch": "gulp scss-watch",
    "postinstall": "cd ../Docusaurus && npm install && rm -rf ../node_modules && mv node_modules ../"
  },
  "devDependencies": {
    "docusaurus": "../Docusaurus/",
    "gulp": "^3.9.1",
    "gulp-changed": "^3.2.0",
    "gulp-cli": "^2.0.1",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-sass": "^4.0.1",
    "gulp-util": "^3.0.8",
    "node-sass": "github:sass/node-sass#v5",
    "npm-run-all": "^4.1.3"
  },
  "dependencies": {}
}
